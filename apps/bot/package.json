  "dependencies": {
    "convex": "^1.14.1",
    "discord.js": "^14.14.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.57.0",
    "@types/node": "^20.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^13.24.0",
    "prettier": "^3.2.5",
    "typescript-eslint": "^7.10.0",
    "typescript": "^5.5.0"
  }
}
